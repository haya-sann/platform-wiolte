  "license": "Apache-2.0",
  "engines": {
    "platformio": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "http://example.com"
  },
  "version": "0.2.0",
  "packageRepositories": [
    "https://dl.bintray.com/platformio/dl-packages/manifest.json",
    "http://dl.platformio.org/packages/manifest.json",
    "https://raw.githubusercontent.com/eerimoq/simba/master/make/platformio/manifest.json",
    {
      "framework-Seeed_STM32F4": [
        {
          "url": "https://github.com/masato-ka/wio-lte-package-repository/raw/master/Seeed_STM32F4_v1.2.2.tar.gz",
          "sha1": "2a88134805bceec5786f3451d8cfe58bbab0f843",
          "version": "1.2.2",
          "system": "*"
        }
      ]
    }

